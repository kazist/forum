        "from": {
            "table": "Forum\\Categories\\Code\\Tables\\Categories",
            "alias": "fc",
            "name": "#__forum_categories"
        },
        "select": [
            "fc.id",
            "fc.forum_id",
            "ff.title AS forum_id_title",
            "fc.title",
            "fc.description",
            "fc.created_by",
            "fc.date_created",
            "fc.modified_by",
            "fc.date_modified"
        ],
        "leftjoin": [
