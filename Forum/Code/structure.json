    "query": {
        "from": {
            "table": "Forum\\Forum\\Code\\Tables\\Forum",
            "alias": "ff",
            "name": "#__forum_forum"
        },
        "select": [
            "ff.id",
            "ff.title",
            "ff.description",
            "ff.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "ff.hit",
            "ff.published",
            "ff.featured",
            "ff.ordering",
            "ff.created_by",
            "uu.name AS created_by_name",
            "ff.date_created",
            "ff.modified_by",
            "uu_8096.name AS modified_by_name",
            "ff.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "ff.image = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ff.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8096",
                "name": "#__users_users",
                "on": "ff.modified_by = uu_8096.id"
            }
        ]
    }
