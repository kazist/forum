    "query": {
        "from": {
            "table": "Forum\\Topics\\Files\\Code\\Tables\\Files",
            "alias": "ftf",
            "name": "#__forum_topics_files"
        },
        "select": [
            "ftf.id",
            "ftf.topic_id",
            "ft.title AS topic_id_title",
            "ftf.file",
            "mm.title AS file_title",
            "mm.file AS file_file",
            "ftf.created_by",
            "ftf.date_created",
            "ftf.modified_by",
            "ftf.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Forum\\Topics\\Code\\Tables\\Topics",
                "alias": "ft",
                "name": "#__forum_topics",
                "on": "ftf.topic_id = ft.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "ftf.file = mm.id"
            }
        ]
    }
