    "query": {
        "from": {
            "table": "Forum\\Topics\\Code\\Tables\\Topics",
            "alias": "ft",
            "name": "#__forum_topics"
        },
        "select": [
            "ft.id",
            "ft.title",
            "ft.description",
            "ft.user_id",
            "uu.name AS user_id_name",
            "ft.category_id",
            "fc.title AS category_id_title",
            "ft.hit",
            "ft.published",
            "ft.moderated",
            "ft.approval",
            "ft.created_by",
            "uu_8784.name AS created_by_name",
            "ft.date_created",
            "ft.modified_by",
            "uu_931.name AS modified_by_name",
            "ft.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ft.user_id = uu.id"
            },
            {
                "table": "Forum\\Categories\\Code\\Tables\\Categories",
                "alias": "fc",
                "name": "#__forum_categories",
                "on": "ft.category_id = fc.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8784",
                "name": "#__users_users",
                "on": "ft.created_by = uu_8784.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_931",
                "name": "#__users_users",
                "on": "ft.modified_by = uu_931.id"
            }
        ]
    }
